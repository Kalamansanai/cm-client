{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "bracketSameLine": false,
  "arrowParens": "always",
  "plugins": ["prettier-plugin-organize-imports"]
}
